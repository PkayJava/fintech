            <column name="annual_fee_on_day" type="SMALLINT"/>
            <column name="annual_fee_next_due_date" type="DATE"/>
            <column name="total_annual_fees_derived" type="DECIMAL(19,6)"/>
        </addColumn>

    </changeSet>
</databaseChangeLog>